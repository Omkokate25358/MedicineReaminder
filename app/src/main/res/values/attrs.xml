<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundedThickGaugeView">
        <!-- Value attributes -->
        <attr name="currentValue" format="integer"/>
        <attr name="totalValue" format="integer"/>
        <attr name="dayOfWeek" format="string"/>
        <attr name="label" format="string"/>

        <!-- Color attributes -->
        <attr name="backgroundColor" format="color"/>
        <attr name="progressColor" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="subtitleColor" format="color"/>
    </declare-styleable>
</resources>